{
	"extends": "@hakatashi/eslint-config/typescript",
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-console": "off",
		"no-undef-init": "off",
		"import/no-unresolved": "off",
		"import/no-namespace": "off",
		"import/extensions": "off"
	}
}
